{
	"name": "richardhj/contao-magickimages",
	"description": "ImageMagick integration for Contao OpenSource CMS",
	"keywords": ["contao", "images", "imagemagick"],
	"type": "contao-module",
	"license": "LGPL-3.0+",
	"authors": [
		{
			"name": "Tristan Lins",
			"email": "tristan.lins@bit3.de",
			"homepage": "http://bit3.de",
			"role": "Developer"
		},
		{
			"name": "Richard Henkenjohann",
			"role": "Developer"
		}
	],
	"support": {
		"email": "info@bit3.de",
		"issues": "https://github.com/richardhj/contao-magickimages/issues",
		"wiki": "http://de.contaowiki.org/MagickImages",
		"irc": "irc://irc.freenode.org/contao.dev",
		"source": "https://github.com/richardhj/contao-magickimages"
	},
	"require": {
		"php": ">=5.3",
		"contao/core": ">=3.1",
		"contao-community-alliance/composer-installer": "*",
		"contao-community-alliance/dependency-container": "*",
		"symfony/process": "2.3.*"
	},
	"replace": {
		"contao-legacy/magickimages": "*"
	},
	"autoload": {
		"psr-0": {
			"MagickImages\\": "src/"
		}
	},
	"extra": {
		"contao": {
			"sources": {
				"contao": "system/modules/magickimages"
			}
		}
	}
